{
  "scripts": {
    "start": "(sleep 2 && open http://localhost:3000) & static -p 3000",
    "browserify-dev": "NODE_ENV=development watchify 14-browserify/src/app.js -t babelify -g envify -o 14-browserify/build/app.js -dv -p livereactload",
    "browserify-build": "NODE_ENV=production browserify 14-browserify/src/app.js -t babelify -g envify | uglifyjs --screw-ie8 -c=dead_code,evaluate,loops,unused > 14-browserify/build/app.js",
    "webpack-dev": "NODE_ENV=development node 15-webpack/dev-server",
    "webpack-build": "NODE_ENV=production webpack --config 15-webpack/webpack.config.js --progress --colors",
    "sr-test": "_mocha 16-testing_shallow_rendering/test/*.js -r babel/register",
    "jsdom-test": "_mocha 17-testing_jsdom/test/**/*.js -r babel/register -r 17-testing_jsdom/setup.js",
    "cover": "babel-node node_modules/.bin/babel-istanbul cover _mocha --include 15-webpack/src/child_component.jsx --dir coverage -- 16-testing_shallow_rendering/test/*.js",
    "flux": "NODE_ENV=development watchify 18-flux_components/src/app.js -t babelify -g envify -o 18-flux_components/build/app.js -dv -p livereactload",
    "top-down": "NODE_ENV=development watchify 20-top_down_rendering/src/app.js -t babelify -g envify -o 20-top_down_rendering/build/app.js -dv -p livereactload",
    "cc": "NODE_ENV=development watchify 21-top_down_rendering_and_container_components/src/app.js -t babelify -g envify -o 21-top_down_rendering_and_container_components/build/app.js -dv -p livereactload",
    "ssr": "NODE_ENV=development nodemon --exec babel-node -- 22-server_rendering/src/server/index.js",
    "ssr-client": "NODE_ENV=development watchify 22-server_rendering/src/components/index.js --s renderState -t babelify -g envify -o 22-server_rendering/src/server/public/javascripts/app.js -dv"
  },
